#import "UILabel+Iconfont.h"

@implementation UILabel (Iconfont)

- (void)setIconText:(NSString *)iconText fontSize:(NSInteger)fontSize
{
    UIFont *iconfont = [YBIconFont fontWithSize:fontSize];
    self.font = iconfont;
    self.text = iconText;
}

@end
